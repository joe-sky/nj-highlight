{
    "name": "html",
    "displayName": "nj-highlight",
    "description": "highlight for nornj",
    "version": "0.2.15",
    "publisher": "yuhongda",
    "engines": {
        "vscode": "^1.19.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "html",
                "aliases": [
                    "nj-html",
                    "html"
                ],
                "extensions": [
                    ".html"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "html",
                "scopeName": "t.html",
                "path": "./syntaxes/nj-html.tmLanguage.json"
            }
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/yuhongda/nj-highlight.git"
    }
}